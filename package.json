  "description": "Google HashCode Runner",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "node server.js",
    "rm-optimal-solution": "rm -rf public/Solution* && rm -f ./optimalSolution.json",
    "pretty": "npx prettier --write \"*.{js,json}\""
