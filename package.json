    "moment": "^2.24.0",
    "shelljs": "^0.8.5",
    "superagent": "^5.2.1",
    "tempy": "^0.4.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "adm-zip": "^0.4.14",
    "body-parser": "^1.19.0",
